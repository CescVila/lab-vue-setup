{"ast":null,"code":"export default {\n  data() {\n    return {\n      firstName: \"Gustavo\",\n      lastName: \"Cerati Martinez\",\n      items: [{\n        title: \"screen\",\n        price: 100\n      }, {\n        title: \"earphone\",\n        price: 30\n      }, {\n        title: \"chair\",\n        price: 150\n      }, {\n        title: \"mouse\",\n        price: 20\n      }, {\n        title: \"coffe cup\",\n        price: 5\n      }]\n    };\n  },\n\n  methods: {\n    changeFavGuitarrist() {\n      this.favGuitarrist2 = \"Cesc Vila\";\n    }\n\n  },\n  computed: {\n    favGuitarrist() {\n      return `${this.firstName} ${this.lastName}`;\n    },\n\n    favGuitarrist2: {\n      get() {\n        return `${this.firstName} ${this.lastName}`;\n      },\n\n      set(value) {\n        const name = value.split(\" \");\n        this.firstName = name[0];\n        this.lastName = name[1];\n      }\n\n    },\n\n    computedItemList() {\n      return this.items.filter(item => item.price < 100);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsBA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,SADN;MAELC,QAAQ,EAAE,iBAFL;MAGLC,KAAK,EAAE,CACL;QAACC,KAAK,EAAC,QAAP;QAAiBC,KAAK,EAAE;MAAxB,CADK,EAEL;QAACD,KAAK,EAAC,UAAP;QAAmBC,KAAK,EAAE;MAA1B,CAFK,EAGL;QAACD,KAAK,EAAC,OAAP;QAAgBC,KAAK,EAAE;MAAvB,CAHK,EAIL;QAACD,KAAK,EAAC,OAAP;QAAgBC,KAAK,EAAE;MAAvB,CAJK,EAKL;QAACD,KAAK,EAAC,WAAP;QAAoBC,KAAK,EAAE;MAA3B,CALK;IAHF,CAAP;EAWD,CAbY;;EAcbC,OAAO,EAAE;IACPC,mBAAmB,GAAG;MACpB,KAAKC,cAAL,GAAqB,WAArB;IACD;;EAHM,CAdI;EAmBbC,QAAQ,EAAE;IACRC,aAAa,GAAE;MACb,OAAQ,GAAE,KAAKT,SAAS,IAAI,KAAKC,QAAS,EAA1C;IACD,CAHO;;IAIRM,cAAc,EAAE;MACdG,GAAG,GAAG;QACN,OAAQ,GAAE,KAAKV,SAAS,IAAI,KAAKC,QAAS,EAA1C;MACC,CAHa;;MAIdU,GAAG,CAACC,KAAD,EAAQ;QACT,MAAMC,IAAG,GAAID,KAAK,CAACE,KAAN,CAAY,GAAZ,CAAb;QACA,KAAKd,SAAL,GAAiBa,IAAI,CAAC,CAAD,CAArB;QACA,KAAKZ,QAAL,GAAeY,IAAG,CAAG,CAAH,CAAlB;MACD;;IARa,CAJR;;IAcTE,gBAAgB,GAAG;MAClB,OAAO,KAAKb,KAAL,CAAWc,MAAX,CAAmBC,IAAD,IAAUA,IAAI,CAACb,KAAL,GAAa,GAAzC,CAAP;IACA;;EAhBQ